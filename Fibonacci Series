#include <stdio.h>

int main() {
    int n1, n2, n3, l, i;
    printf("Enter the Fiboncci Output: ");
    scanf("%d", &l);
    printf("Enter N1: ");
    scanf("%d", &n1);
    printf("Enter N2: ");
    scanf("%d", &n2);
    for(i=0; i<l; i++) 
    {
        n3 = n1 + n2;
        printf("%d\t", n3);
        n1 = n2;
        n2 = n3;
    }
    return 0;
}
